DEPTH = 256;                   -- The size of data in bits
WIDTH = 32;                    -- The size of memory in words
ADDRESS_RADIX = DEC;          -- The radix for address values
DATA_RADIX = BIN;             -- The radix for data values
CONTENT                       -- start of (address : data pairs)
BEGIN
 
0 : 00000000000000000000000000000000;       --nop
1 : 10001100000000100000000000000001;       -- LW 
2 : 10001100000000010000000000000100;       -- LW        
3 : 00000000001000100001100000100000;       -- SOMA  R1 com R2 em R3 
4 : 00000000001000100010000000100010;       -- SUB   R1 com R2 em R4
5 : 00000000001000100001100000100100;       -- AND   R1 com R2 em R3
6 : 00000000001000100010000000100101;       -- OR    R1 com R2 em R4 
7 : 00000000010000010001100000101010;       -- SLT   R1 com R2 em R3
8 : 10101100000000010000000100000001;       -- SW(ledr0)
9 : 00010000100001000000000000000001;       -- BEQ   R4 com R4
10 : 00001000000000000000000000001011;       -- JUMP  -> 11
11: 00001000000000000000000000001001;       -- JUMP  -> 9
[12..255]: 00000000000000000000000000000000; -- Others
END;